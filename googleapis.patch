+        "//google/ads/googleads/v15/resources:resources_go_proto",
+        "//google/ads/googleads/v15/services:services_go_proto",
+    ],
+)
+
+go_test(
+    name = "googleads_go_gapic_test",
+    srcs = [":googleads_go_gapic_srcjar_test"],
+    embed = [":googleads_go_gapic"],
+    importpath = "google.golang.org/google/ads/googleads/v15",
+)
+
+# Open Source Packages
+go_gapic_assembly_pkg(
+    name = "gapi-ads-googleads-v15-go",
+    deps = [
+        ":googleads_go_gapic",
+        "//google/ads/googleads/v15/common:common_go_proto",
+        "//google/ads/googleads/v15/enums:enums_go_proto",
+        "//google/ads/googleads/v15/errors:errors_go_proto",
+        "//google/ads/googleads/v15/resources:resources_go_proto",
+        "//google/ads/googleads/v15/services:services_go_proto",
+    ],
+)
